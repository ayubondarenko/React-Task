{
  "name": "react-and-bootstrap",
  "version": "1.0.0",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "description": "An example of how to build static website using bootstrap",
  "scripts": {
    "start": "npm run server",
    "build": "webpack -p --display-error-details --color ",
    "server": "node ./server/server.js",
    "dev": "npm run server "
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "jquery": "^3.3.1",
    "json-server": "^0.12.1",
    "redux-devtools-extension": "^2.13.2",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^3.10.0",
    "webpack-icons-installer": "^1.0.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0",
    "moment": "^2.20.1",
    "popper.js": "^1.12.9",
    "react": "^16.2.0",
    "react-datepicker": "^1.1.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  }
}
